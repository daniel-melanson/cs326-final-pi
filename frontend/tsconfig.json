{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["DOM", "ESNext"],
    "jsx": "react",
    "jsxFactory": "Enact.createElement",
    "module": "commonjs",
    "baseUrl": "./src",
    "types": ["bootstrap"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
